{
    "plugins": [
        "latex-codecogs",
        "theme-latex",
        "-highlight",
        "prism",
        "prism-themes",
        "ace",
        "include-codeblock",
        "include-highlight",
        "sectionx",
        "github-buttons",
        "anchor-navigation",
        "anchor-navigation-ex",
        "anchors",
        "anchors-wrapper",
        "expandable-chapters",
        "atoc",
        "image-captions",
        "image-wrapper",
        "simple-page-toc",
        "toggle-chapters"
    ],
    "output": null,
    "pluginsConfig": {
        "fontsettings": {
            "theme": "sepia",
            "family": "sans",
            "size": 3
        },
        "github": {
          "url": "https://github.com/heluka/he-book"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-solarizedlight.css"
            ]
        },
        "github-buttons": {
            "repo": "heluka/he-book",
            "types": [
                "star",
                "watch"
            ],
            "size": "large"
        }
    },
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "author": "heluka",
    "pdf": {
        "headerTemplate": null,
        "margin": {
            "top": 36,
            "bottom": 36
        },
        "footerTemplate": null,
        "pageNumbers": false
    },
    "title": "linux底层基础学习笔记",
    "language": "zh-hans",
    "description": "用来记录关于linux汇编语言编程、ELF文件结构、动态链接、共享库等方面内容的学习笔记"
}